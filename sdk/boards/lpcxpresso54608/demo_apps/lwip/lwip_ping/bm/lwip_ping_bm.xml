  <example id="lpcxpresso54608_lwip_ping_bm" name="lwip_ping_bm" category="demo_apps/lwip" dependency="platform.drivers.clock.LPC54608 platform.drivers.common.LPC54608 platform.drivers.reset.LPC54608 platform.devices.LPC54608_CMSIS.LPC54608 platform.devices.LPC54608_startup.LPC54608 platform.drivers.flexcomm_usart.LPC54608 platform.drivers.flexcomm.LPC54608 platform.drivers.lpc_iocon.LPC54608 platform.utilities.debug_console.LPC54608 platform.drivers.lpc_gpio.LPC54608 platform.drivers.emc.LPC54608 platform.utilities.assert.LPC54608 middleware.tcpip.lwip.contrib.ping.LPC54608 middleware.tcpip.lwip.LPC54608 platform.drivers.lpc_enet.LPC54608 platform.drivers.power.LPC54608 platform.drivers.powerlib.LPC54608" toolchain="iar mdk mcuxpresso armgcc">
    <projects>
      <project type="com.crt.advproject.projecttype.exe" nature="org.eclipse.cdt.core.cnature"/>
    </projects>
    <toolchainSettings>
      <!-- Curently, mcux just supports one common settings set for both debug and release -->
      <toolchainSetting id_refs="com.nxp.mcuxpresso">
        <!-- asm compiler -->
        <!-- gcc compiler -->
        <option id="com.crt.advproject.c.misc.dialect" type="enum">
          <value>com.crt.advproject.misc.dialect.gnu99</value>
        </option>
        <option id="com.crt.advproject.gcc.exe.debug.option.debugging.level" type="enum">
          <value>gnu.c.debugging.level.default</value>
        </option>
        <option id="com.crt.advproject.gcc.exe.release.option.debugging.level" type="enum">
          <value>gnu.c.debugging.level.default</value>
        </option>
        <option id="com.crt.advproject.gcc.exe.debug.option.optimization.level" type="enum">
          <value>gnu.c.optimization.level.none</value>
        </option>
        <option id="com.crt.advproject.gcc.exe.release.option.optimization.level" type="enum">
          <value>gnu.c.optimization.level.none</value>
        </option>
        <option id="gnu.c.compiler.option.preprocessor.def.symbols" type="stringList">
          <value>CPU_LPC54608</value>
          <value>__USE_CMSIS</value>
          <value>CPU_LPC54608J512ET180=1</value>
          <value>PRINTF_ADVANCED_ENABLE=1</value>
        </option>
        <option id="gnu.c.compiler.option.optimization.flags" type="string">
          <value>-fno-common</value>
        </option>
        <option id="gnu.c.compiler.option.misc.other" type="string">
          <value>-c  -ffunction-sections  -fdata-sections  -ffreestanding  -fno-builtin</value>
        </option>
        <!-- gcc linker -->
        <option id="gnu.c.link.option.nostdlibs" type="boolean">
          <value>true</value>
        </option>
        <option id="gnu.c.link.option.other" type="stringList">
          <value>--gc-sections</value>
          <value>--defsym=__ram_vector_table__=1</value>
        </option>
        <option id="com.crt.advproject.link.memory.heapAndStack" type="string">
          <value>&amp;Heap:Default;Default;0x1000&amp;Stack:Default;Default;0x1000</value>
        </option>
      </toolchainSetting>
    </toolchainSettings>
    <include_paths>
      <include_path path="lwip/contrib/apps"/>
      <include_path path="lwip/port"/>
      <include_path path="lwip/src"/>
      <include_path path="lwip/src/include"/>
    </include_paths>
    <source path="boards/lpcxpresso54608/demo_apps/lwip/lwip_ping/bm" target_path="board" type="c_include">
      <files mask="board.h"/>
      <files mask="clock_config.h"/>
      <files mask="fsl_phy.h"/>
      <files mask="pin_mux.h"/>
    </source>
    <source path="boards/lpcxpresso54608/demo_apps/lwip/lwip_ping/bm" target_path="board" type="src">
      <files mask="board.c"/>
      <files mask="clock_config.c"/>
      <files mask="fsl_phy.c"/>
      <files mask="pin_mux.c"/>
    </source>
    <source path="boards/lpcxpresso54608/demo_apps/lwip/lwip_ping/bm" target_path="doc" type="doc" toolchain="iar mdk kds mcuxpresso">
      <files mask="readme.txt"/>
    </source>
    <source path="boards/lpcxpresso54608/demo_apps/lwip/lwip_ping/bm" target_path="source" type="c_include">
      <files mask="lwipopts.h"/>
    </source>
    <source path="boards/lpcxpresso54608/demo_apps/lwip/lwip_ping/bm" target_path="source" type="src">
      <files mask="lwip_ping_bm.c"/>
    </source>
